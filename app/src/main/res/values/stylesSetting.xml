<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TextStyleSetting">
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:textColor">@color/black_setting</item>
        <item name="android:textSize">22sp</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
    </style>
    <style name="TextStyle_menu">
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:textColor">@color/black_setting</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">@dimen/margin_start</item>
        <item name="android:layout_marginTop">@dimen/layout_margin_setting</item>
        <item name="android:layout_marginBottom">@dimen/layout_margin_setting</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
    </style>
    <style name="LinerStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
    </style>
    <style name="FrameStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="IconStyleButton">
        <item name="android:layout_width">@dimen/margin_bottom_setting</item>
        <item name="android:layout_height">@dimen/margin_bottom_setting</item>
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:layout_marginEnd">@dimen/margin_setting_end</item>
    </style>
</resources>